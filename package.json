{
  "name": "user_ts",
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "https://github.com/felipecal/Users-TS.git",
  "author": "Felipe <fcalldas@gmail.com>",
  "license": "MIT",
  "scripts": {
    "pretest": "NODE_ENV=development sequelize db:migrate:undo:all",
    "test": "NODE_ENV=development sequelize db:migrate && sequelize db:seed:all",
    "dev": "yarn ts-node-dev --respawn --transpile-only --watch src/**/*.gql src/index.ts",
    "build:babel": "babel src --extensions .ts --copy-files --out-dir distBabel",
    "build:tsc": "tsc -p .",
    "start": "concurrently \"yarn dev\" \"yarn watch\""
  },
  "dependencies": {
    "@apollo/federation": "^0.36.1",
    "@graphql-tools/load-files": "^6.6.1",
    "apollo-server": "^3.10.2",
    "apollo-server-core": "^3.10.2",
    "apollo-server-express": "^3.10.2",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "graphql": "^16.6.0",
    "graphql-fields": "^2.0.3",
    "graphql-tools": "^8.3.4",
    "path": "^0.12.7",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.21.4",
    "sequelize-typescript": "^2.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/node": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "concurrently": "^7.3.0",
    "sequelize-cli": "^6.4.1",
    "sqlite3": "^5.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.2"
  }
}
